{
	"version": "2.0.0",
	"tasks": [
		
		{
			"type": "cppbuild",
			"label": "g++.exe build active file",
			"command": "H:\\MinGW\\bin\\g++.exe",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
